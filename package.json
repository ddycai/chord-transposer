{
  "name": "chord-transposer",
  "version": "3.0.1",
  "description": "Transposes musical chords in arbitrary text.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "ts-enums": "0.0.6",
    "xregexp": "^4.4.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.2",
    "@types/node": "^12.19.7",
    "jasmine": "^3.6.3",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7",
    "typescript-formatter": "^7.2.2"
  },
  "scripts": {
    "build": "npx tslint src/* && tsfmt -r && tsc",
